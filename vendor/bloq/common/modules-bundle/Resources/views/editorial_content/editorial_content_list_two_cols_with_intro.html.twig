{% if contents | length > 0 %}
<section class="clear mas-contenido">
    <h1 class="titulo-seccion">{{ title }}</h1>
    {% for content in contents %}
    <article class="medio pad">
        <a href="{{ content.urls().0.url() }}" class="imagen"><img src="{{ content.multimedias.0.getFrontImageUrl() | optimized('rectangle_article_in_cover') }}" alt="{% if content.multimedias.0.description is defined and content.multimedias.0.description | length > 0 %}{{ content.multimedias.0.description }}{% else %}{{ content.title }}{% endif %}"></a>
        <h1 class="titulo tam24"><a href="{{ content.urls().0.url() }}">{{ content.title }}</a></h1>
        <p class="entradilla">{{ content.intro }}</p>
    </article>
    {% endfor %}
</section>
{% endif %}
